+    <script>
+        let portfolio = [
+            { symbol: 'BTC', name: 'Bitcoin', amount: 0.5, avgPrice: 45000 },
+            { symbol: 'ETH', name: 'Ethereum', amount: 5, avgPrice: 2800 },
+            { symbol: 'BNB', name: 'Binance Coin', amount: 10, avgPrice: 350 },
+            { symbol: 'SOL', name: 'Solana', amount: 30, avgPrice: 80 },
+            { symbol: 'ADA', name: 'Cardano', amount: 1000, avgPrice: 0.45 },
+            { symbol: 'DOT', name: 'Polkadot', amount: 50, avgPrice: 12 },
+            { symbol: 'MATIC', name: 'Polygon', amount: 500, avgPrice: 0.9 }
+        ];
+
+        let prices = {
+            BTC: 52000,
+            ETH: 3100,
+            BNB: 420,
+            SOL: 110,
+            ADA: 0.52,
+            DOT: 14.5,
+            MATIC: 1.1
+        };
+
+        let transactions = [
+            { type: 'buy', symbol: 'BTC', amount: 0.5, price: 45000, date: '2024-01-15' },
+            { type: 'buy', symbol: 'ETH', amount: 5, price: 2800, date: '2024-01-20' },
+            { type: 'buy', symbol: 'SOL', amount: 30, price: 80, date: '2024-02-01' },
+            { type: 'buy', symbol: 'ADA', amount: 1000, price: 0.45, date: '2024-02-10' },
+            { type: 'sell', symbol: 'BTC', amount: 0.1, price: 51000, date: '2024-02-15' },
+            { type: 'buy', symbol: 'DOT', amount: 50, price: 12, date: '2024-02-20' },
+            { type: 'buy', symbol: 'MATIC', amount: 500, price: 0.9, date: '2024-02-25' }
+        ];
+
+        function showPage(pageId, linkEl) {
+            document.querySelectorAll('.page').forEach(page => page.classList.remove('active'));
+            document.getElementById(pageId).classList.add('active');
+
+            document.querySelectorAll('.nav-link').forEach(link => link.classList.remove('active'));
+            if (linkEl) linkEl.classList.add('active');
+
+            if (pageId === 'home') updateHomePage();
+            if (pageId === 'analytics') updateAnalyticsPage();
+            if (pageId === 'history') updateHistoryPage();
+        }
+
+        function updatePrices() {
+            Object.keys(prices).forEach(sym => {
+                const change = (Math.random() * 10 - 5) / 100;
+                prices[sym] = prices[sym] * (1 + change);
+            });
+
+            updateAllPages();
+            showNotification('–¶–µ–Ω—ã –æ–±–Ω–æ–≤–ª–µ–Ω—ã', 'success');
+        }
+
+        function addRandomTransaction() {
+            const symbols = ['BTC', 'ETH', 'BNB', 'SOL', 'ADA', 'DOT', 'MATIC'];
+            const types = ['buy', 'sell'];
+
+            const symbol = symbols[Math.floor(Math.random() * symbols.length)];
+            const type = types[Math.floor(Math.random() * types.length)];
+
+            const asset = portfolio.find(a => a.symbol === symbol);
+
+            if (type === 'buy') {
+                const amount = Math.random() * 2 + 0.1;
+                const price = prices[symbol] * (0.9 + Math.random() * 0.2);
+
+                if (asset) {
+                    const totalValue = asset.amount * asset.avgPrice + amount * price;
+                    asset.amount += amount;
+                    asset.avgPrice = totalValue / asset.amount;
+                } else {
+                    portfolio.push({ symbol, name: getAssetName(symbol), amount, avgPrice: price });
+                }
+
+                transactions.unshift({ type: 'buy', symbol, amount, price, date: new Date().toLocaleDateString('ru-RU') });
+            } else if (asset && asset.amount > 0) {
+                const amount = Math.random() * asset.amount * 0.5;
+                const price = prices[symbol] * (0.9 + Math.random() * 0.2);
+
+                asset.amount -= amount;
+
+                if (asset.amount < 0.001) {
+                    portfolio = portfolio.filter(a => a.symbol !== symbol);
+                }
+
+                transactions.unshift({ type: 'sell', symbol, amount, price, date: new Date().toLocaleDateString('ru-RU') });
+            }
+
+            updateAllPages();
+            showNotification('–°–¥–µ–ª–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∞', 'success');
+        }
+
+        function resetPortfolio() {
+            portfolio = [
+                { symbol: 'BTC', name: 'Bitcoin', amount: 0.5, avgPrice: 45000 },
+                { symbol: 'ETH', name: 'Ethereum', amount: 5, avgPrice: 2800 },
+                { symbol: 'BNB', name: 'Binance Coin', amount: 10, avgPrice: 350 },
+                { symbol: 'SOL', name: 'Solana', amount: 30, avgPrice: 80 },
+                { symbol: 'ADA', name: 'Cardano', amount: 1000, avgPrice: 0.45 },
+                { symbol: 'DOT', name: 'Polkadot', amount: 50, avgPrice: 12 },
+                { symbol: 'MATIC', name: 'Polygon', amount: 500, avgPrice: 0.9 }
+            ];
+
+            prices = { BTC: 52000, ETH: 3100, BNB: 420, SOL: 110, ADA: 0.52, DOT: 14.5, MATIC: 1.1 };
+
+            transactions = [
+                { type: 'buy', symbol: 'BTC', amount: 0.5, price: 45000, date: '2024-01-15' },
+                { type: 'buy', symbol: 'ETH', amount: 5, price: 2800, date: '2024-01-20' },
+                { type: 'buy', symbol: 'SOL', amount: 30, price: 80, date: '2024-02-01' },
+                { type: 'buy', symbol: 'ADA', amount: 1000, price: 0.45, date: '2024-02-10' },
+                { type: 'sell', symbol: 'BTC', amount: 0.1, price: 51000, date: '2024-02-15' },
+                { type: 'buy', symbol: 'DOT', amount: 50, price: 12, date: '2024-02-20' },
+                { type: 'buy', symbol: 'MATIC', amount: 500, price: 0.9, date: '2024-02-25' }
+            ];
+
+            updateAllPages();
+            showNotification('–ü–æ—Ä—Ç—Ñ–µ–ª—å —Å–±—Ä–æ—à–µ–Ω', 'info');
+        }
+
+        function getAssetName(symbol) {
+            const names = {
+                BTC: 'Bitcoin',
+                ETH: 'Ethereum',
+                BNB: 'Binance Coin',
+                SOL: 'Solana',
+                ADA: 'Cardano',
+                DOT: 'Polkadot',
+                MATIC: 'Polygon'
+            };
+            return names[symbol] || symbol;
+        }
+
+        function showNotification(message) {
+            console.log(message);
+        }
+
+        function updateAllPages() {
+            updateHomePage();
+            updateAnalyticsPage();
+            updateHistoryPage();
+        }
+
+        function updateHomePage() {
+            let totalValue = 0;
+            let totalCost = 0;
+            let bestAsset = null;
+            let bestChange = -Infinity;
+
+            portfolio.forEach(asset => {
+                const currentPrice = prices[asset.symbol] || 0;
+                const value = asset.amount * currentPrice;
+                const cost = asset.amount * asset.avgPrice;
+
+                totalValue += value;
+                totalCost += cost;
+
+                const change = ((currentPrice - asset.avgPrice) / asset.avgPrice) * 100;
+                if (change > bestChange) {
+                    bestChange = change;
+                    bestAsset = asset;
+                }
+            });
+
+            const profit = totalValue - totalCost;
+            const profitPercent = totalCost > 0 ? (profit / totalCost) * 100 : 0;
+
+            document.getElementById('totalValue').textContent = '$' + totalValue.toLocaleString(undefined, { maximumFractionDigits: 0 });
+            document.getElementById('totalProfit').textContent = (profit >= 0 ? '+' : '') + '$' + profit.toLocaleString(undefined, { maximumFractionDigits: 0 });
+            document.getElementById('profitPercent').textContent = (profitPercent >= 0 ? '+' : '') + profitPercent.toFixed(1) + '%';
+            document.getElementById('assetsCount').textContent = portfolio.length;
+            document.getElementById('totalChange').textContent = '–æ–±–Ω–æ–≤–ª–µ–Ω–æ —Ç–æ–ª—å–∫–æ —á—Ç–æ';
+
+            if (bestAsset) {
+                document.getElementById('bestAsset').textContent = bestAsset.symbol;
+                document.getElementById('bestAssetChange').textContent = (bestChange >= 0 ? '+' : '') + bestChange.toFixed(1) + '%';
+                document.getElementById('bestAssetChange').className = 'stat-change ' + (bestChange >= 0 ? '' : 'negative');
+            }
+
+            let tableHtml = '';
+            portfolio.sort((a, b) => prices[b.symbol] * b.amount - prices[a.symbol] * a.amount);
+
+            portfolio.forEach(asset => {
+                const currentPrice = prices[asset.symbol] || 0;
+                const value = asset.amount * currentPrice;
+                const assetProfit = (currentPrice - asset.avgPrice) * asset.amount;
+                const assetProfitPercent = ((currentPrice - asset.avgPrice) / asset.avgPrice) * 100;
+
+                tableHtml += `
+                    <tr>
+                        <td><strong>${asset.symbol}</strong> ${asset.name}</td>
+                        <td>${asset.amount.toFixed(4)}</td>
+                        <td>$${currentPrice.toFixed(2)}</td>
+                        <td>$${value.toFixed(2)}</td>
+                        <td>$${asset.avgPrice.toFixed(2)}</td>
+                        <td class="${assetProfit >= 0 ? 'positive' : 'negative'}">
+                            ${assetProfit >= 0 ? '+' : ''}$${assetProfit.toFixed(2)} (${assetProfitPercent >= 0 ? '+' : ''}${assetProfitPercent.toFixed(1)}%)
+                        </td>
+                    </tr>
+                `;
+            });
+
+            document.getElementById('portfolioBody').innerHTML = tableHtml;
+
+            let chartHtml = '';
+            const maxValue = Math.max(...portfolio.map(a => prices[a.symbol] * a.amount));
+
+            portfolio.slice(0, 5).forEach(asset => {
+                const value = prices[asset.symbol] * asset.amount;
+                const percent = (value / maxValue) * 100;
+
+                chartHtml += `
+                    <div class="bar-wrapper">
+                        <div class="bar" style="height: ${percent * 2}px;"></div>
+                        <div class="bar-label">${asset.symbol}</div>
+                        <div class="bar-value">$${value.toFixed(0)}</div>
+                    </div>
+                `;
+            });
+
+            document.getElementById('homeChart').innerHTML = chartHtml;
+        }
+
+        function updateAnalyticsPage() {
+            let totalInvestment = 0;
+            let totalValue = 0;
+            let bestProfit = -Infinity;
+            let bestAsset = null;
+            let worstProfit = Infinity;
+            let worstAsset = null;
+
+            portfolio.forEach(asset => {
+                const currentPrice = prices[asset.symbol] || 0;
+                const investment = asset.amount * asset.avgPrice;
+                const value = asset.amount * currentPrice;
+
+                totalInvestment += investment;
+                totalValue += value;
+
+                const profit = ((currentPrice - asset.avgPrice) / asset.avgPrice) * 100;
+
+                if (profit > bestProfit) {
+                    bestProfit = profit;
+                    bestAsset = asset;
+                }
+                if (profit < worstProfit) {
+                    worstProfit = profit;
+                    worstAsset = asset;
+                }
+            });
+
+            const roi = totalInvestment > 0 ? ((totalValue - totalInvestment) / totalInvestment) * 100 : 0;
+
+            document.getElementById('mostProfitable').textContent = bestAsset ? bestAsset.symbol : '‚Äî';
+            document.getElementById('mostProfitablePercent').textContent = (bestProfit >= 0 ? '+' : '') + bestProfit.toFixed(1) + '%';
+            document.getElementById('leastProfitable').textContent = worstAsset ? worstAsset.symbol : '‚Äî';
+            document.getElementById('leastProfitablePercent').textContent = worstProfit.toFixed(1) + '%';
+            document.getElementById('totalInvestment').textContent = '$' + totalInvestment.toLocaleString(undefined, { maximumFractionDigits: 0 });
+            document.getElementById('roi').textContent = (roi >= 0 ? '+' : '') + roi.toFixed(1) + '%';
+
+            let tableHtml = '';
+            portfolio.forEach(asset => {
+                const currentPrice = prices[asset.symbol] || 0;
+                const investment = asset.amount * asset.avgPrice;
+                const value = asset.amount * currentPrice;
+                const profit = value - investment;
+                const profitPercent = ((currentPrice - asset.avgPrice) / asset.avgPrice) * 100;
+                const share = (value / totalValue) * 100;
+
+                tableHtml += `
+                    <tr>
+                        <td><strong>${asset.symbol}</strong> ${asset.name}</td>
+                        <td>$${investment.toFixed(2)}</td>
+                        <td>$${value.toFixed(2)}</td>
+                        <td class="${profit >= 0 ? 'positive' : 'negative'}">${profit >= 0 ? '+' : ''}$${profit.toFixed(2)}</td>
+                        <td class="${profitPercent >= 0 ? 'positive' : 'negative'}">${profitPercent >= 0 ? '+' : ''}${profitPercent.toFixed(1)}%</td>
+                        <td>${share.toFixed(1)}%</td>
+                    </tr>
+                `;
+            });
+
+            document.getElementById('analyticsBody').innerHTML = tableHtml;
+
+            let recommendations = '';
+            if (bestProfit > 20) {
+                recommendations += `<p style="color: #4caf50; margin: 10px 0;">‚úÖ –†–∞—Å—Å–º–æ—Ç—Ä–∏—Ç–µ —Ñ–∏–∫—Å–∞—Ü–∏—é –ø—Ä–∏–±—ã–ª–∏ –ø–æ ${bestAsset.symbol}</p>`;
+            }
+            if (worstProfit < -15) {
+                recommendations += `<p style="color: #e94560; margin: 10px 0;">‚ö†Ô∏è –£—Å—Ä–µ–¥–Ω—è–π—Ç–µ –ø–æ–∑–∏—Ü–∏—é –ø–æ ${worstAsset.symbol} –∏–ª–∏ —Ñ–∏–∫—Å–∏—Ä—É–π—Ç–µ —É–±—ã—Ç–æ–∫</p>`;
+            }
+            if (portfolio.length < 3) {
+                recommendations += '<p style="color: #f39c12; margin: 10px 0;">üìä –î–æ–±–∞–≤—å—Ç–µ –∞–∫—Ç–∏–≤—ã –¥–ª—è –¥–∏–≤–µ—Ä—Å–∏—Ñ–∏–∫–∞—Ü–∏–∏</p>';
+            } else {
+                recommendations += '<p style="color: #4caf50; margin: 10px 0;">‚úÖ –•–æ—Ä–æ—à–∞—è –¥–∏–≤–µ—Ä—Å–∏—Ñ–∏–∫–∞—Ü–∏—è</p>';
+            }
+
+            document.getElementById('recommendations').innerHTML = recommendations || '<p>–ù–µ—Ç —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π</p>';
+        }
+
+        function updateHistoryPage() {
+            filterHistory();
+        }
+
+        function filterHistory() {
+            const search = document.getElementById('historySearch').value.toLowerCase();
+            const type = document.getElementById('historyType').value;
+
+            let filtered = transactions;
+
+            if (type !== 'all') {
+                filtered = filtered.filter(t => t.type === type);
+            }
+
+            if (search) {
+                filtered = filtered.filter(t => t.symbol.toLowerCase().includes(search));
+            }
+
+            let tableHtml = '';
+            filtered.forEach(t => {
+                tableHtml += `
+                    <tr>
+                        <td>${t.date}</td>
+                        <td><span class="badge ${t.type}">${t.type === 'buy' ? '–ü–û–ö–£–ü–ö–ê' : '–ü–†–û–î–ê–ñ–ê'}</span></td>
+                        <td><strong>${t.symbol}</strong></td>
+                        <td>${t.amount.toFixed(4)}</td>
+                        <td>$${t.price.toFixed(2)}</td>
+                        <td>$${(t.amount * t.price).toFixed(2)}</td>
+                    </tr>
+                `;
+            });
+
+            if (filtered.length === 0) {
+                tableHtml = '<tr><td colspan="6" style="text-align: center; padding: 30px;">–ù–µ—Ç —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π</td></tr>';
+            }
+
+            document.getElementById('historyBody').innerHTML = tableHtml;
+        }
+
+        window.onload = function() {
+            updateHomePage();
+            updateAnalyticsPage();
+            updateHistoryPage();
+        };
+    </script>
+</body>
+</html>
 
